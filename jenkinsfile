pipeline {
  agent any
  tools {
    maven 'Maven'
  }
  stages {
    stage('Initialize'){
      steps{
         sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
            ''' 
      }
    }
    stage ('Great'){
      steps{
        sh '''
                    echo "Hello World!"
                  
            ''' 
        }
    }
     stage ('Build'){
      steps{
        sh 'mvn clean package'
        }
    }
  }
}
